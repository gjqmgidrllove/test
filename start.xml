<?xml version="1.0" encoding="UTF-8"?>
<MOML>
    <THEMES>
        <THEMES ID ="style1" style="round" padding="10" margin="5"/>
        </THEMES>
    <UILAYOUT portrait="320,460" landscape="320,460">
        <WINDOW layout="0,0,320,460" defaultImg="#ffffff">
            <LABEL layout="10,10,auto,auto" text="Hello MOML" />
          <!--  <WEBVIEW layoyt="10,60,300,300" src = "http://www.naver.com">-->
                
                <CAMERA layoyt="10,60,300,300" />
                <WINDOW layout="10,300,300,auto" align="linear">
                    <STYLEBUTTON themeId="style1" text = "open" img="lightgreen" onClick="cam.open"/>
                    <STYLEBUTTON themeId="style1" text = "close" img="lightblue" onClick="cam.close"/>
                    <STYLEBUTTON themeId="style1" text = "front"  img="yellow" onClick="cam.type='front'"/>
                    <STYLEBUTTON themeId="style1" text = "back" img="#ff00ff" onClick="cam.type='back'"/>
                     <STYLEBUTTON themeId="style1" text = "take"  onClick="cam.takePicture"/>

        </WINDOW>
    </UILAYOUT>
</MOML>
